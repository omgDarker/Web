.user-login img{
	max-width:100%;
}
.user-register{
	background-attachment: fixed;
	background-size:cover;
}
.user-register img{
	max-width:100%;
}
.user-register .form-control{
	font-size:12px;
}
.user-register .nav-tabs {
	border-bottom:0px;
}
.user-register .nav-tabs>li{
	width:50%;
}
.user-register .nav-tabs>li>a{
	border: 0px!important;
	border-radius: 0px!important;
	color: #b5b5b5;
	background: rgba(0,0,0,.4);
	background: #484949\9;
	text-align:center;
}
.user-register .nav-tabs>li>a:hover {
	color: #B20000;
}

.user-register .nav-tabs>li.active>a,.user-register .nav-tabs>li.active>a:hover,.user-register .nav-tabs>li.active>a:focus
{
	color: #FFFFFF;
	font-weight:bold;
	background-color: transparent;
}
.user-register .panel{
	font-family: Tahoma,"\5b8b\4f53",Arial,Helvetica,Arial,sans-serif;
	background: rgba(0,0,0,.5);
	background: #484949\9;
	border: 0px;
	color:#bfbfbf;
	font-size:12px;
	border-radius: 0px;
	margin-bottom:0px;
	min-height:360px;
}
.user-register .panel a{color:#bfbfbf;}
.user-register .panel a:hover{color:#B20000;}
.user-register .panel-body{
	padding:0px;
}
