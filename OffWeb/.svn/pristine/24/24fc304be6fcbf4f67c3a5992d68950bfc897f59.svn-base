/********导航条*************************/
.bg-color {
  background: #b20000;
}
.navbar-header {
  position: relative;
}
.navbar-brand {
  position: relative;
  width: 250px;
  z-index: 999;
  height: auto !important;
  color: #fff !important;
  background: #b20000;
  color: #fff !important;
}
.navbar-brand .navbar-brand-title {
  color: #fff !important;
  cursor: pointer;
}
.navbar-brand .iconfont {
  position: absolute;
  right: 10px;
  top: 50%;
  margin-top: -10px;
  font-size: 16px;
}
.navbar-brand:hover .iconfont {
  -moz-transform: scaley(-1);
  -webkit-transform: scaley(-1);
  -o-transform: scaley(-1);
  transform: scaley(-1);
  filter: flipv();
  transition: 0.05s;
}
/***已经采用js替代，添加了延迟功能
.navbar-brand:hover>.nav-category-section{
	display:block;
}***/
.nav-category-section {
  position: absolute;
  top: 51px;
  left: 0px;
  width: 100%;
  height: 426px;
  padding-left: 1px;
  display: none;
}
.nav-category-section > ul {
  width: 100%;
}
.nav-category-section > ul > li {
  position: relative;
  height: 72px;
  border: 1px solid #dfdfdf;
  color: #6d6d6d;
  margin-top: -1px;
  margin-left: -1px;
  line-height: 25px;
  padding: 10px;
  padding-left: 20px;
  background: #fff;
}
.nav-category-section .title {
  display: block;
  color: #B20000;
  font-size: 16px;
  text-decoration: none;
  white-space: nowrap;
  text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
  overflow: hidden;
}
.nav-category-section .content {
  white-space: nowrap;
  text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
  overflow: hidden;
}
.nav-category-section a {
  color: #6d6d6d;
  text-decoration: none;
  font-size: 13px;
  margin-right: 7px;
}
.nav-category-section .content a:hover {
  color: #333;
}
.nav-category-section > ul > li:hover {
  border-left: 2px solid #B20000;
  z-index: 101;
}
.nav-category-sub {
  width: 100%;
  display: none;
  position: absolute;
  top: 0px;
  z-index: 100;
  margin-left: 0px;
  margin-top: -1px;
  background: #fff;
  border: 1px solid #dfdfdf;
  display: none;
}
.nav-category-sub img {
  height: 40px;
}
.nav-category-sub > ul {
  padding-top: 1px;
}
.nav-category-sub > ul > li {
  position: relative;
  height: 60px;
  border-left: 0px;
  color: #6d6d6d;
  margin-top: -1px;
  line-height: 60px;
  width: 100%;
  padding-left: 20px;
  font-size: 14px;
  white-space: nowrap;
  text-overflow: ellipsis;
  -o-text-overflow: ellipsis;
  overflow: hidden;
}
.nav-category-sub > ul > li > img {
  width: 40px;
  height: 40px;
  margin-right: 10px;
}
.nav-category-sub > ul > li:hover {
  background: #eee;
}
.nav-category-section ul li:hover > .nav-category-sub {
  display: block;
}
.index-nav .nav-category-section {
  display: block !important;
}
/***************搜索:search-bar************************/
