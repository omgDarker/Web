/**********在线客服**********/
.custom-service {
  padding-right: 2px;
  background: #000;
  width: 36px;
  min-height: 100px;
  height: 179px;
  position: fixed;
  right: -2px;
  top: 260px;
  color: #AAA;
  z-index: 1000;
}
.custom-service li {
  position: relative;
  height: 31px;
  line-height: 31px;
  cursor: pointer;
  border-top: 1px dotted #333;
  border-bottom: 1px dotted #333;
  margin-top: -1px;
}
.custom-service li .content {
  padding: 5px 10px 5px 10px;
  min-width: 100px;
  font-size: 12px;
  line-height: 20px;
  color: #FFF;
  right: 34px;
}
.custom-service li .content a {
  color: #FFF;
}
.custom-service li div {
  display: none;
  position: absolute;
  top: 0px;
  background: #333;
}
.custom-service li:hover {
  background: #B20000;
  border: 0px;
  border-top: 1px solid #B20000;
  border-bottom: 1px solid #B20000;
  z-index: 1;
}
.custom-service li:hover div {
  display: block;
}
.custom-service li:hover .triangle {
  height: 26px;
  line-height: 26px;
  padding-right: 0px;
  right: 22px!important;
  background: transparent;
  color: #333;
}
/**异步验证提示****/
.form-control-feedback {
  right: 0px!important;
}
.glyphicon-ok,
.glyphicon-remove {
  display: none;
}
.has-success .glyphicon-ok {
  display: block;
}
.has-error .glyphicon-remove {
  display: block;
}
/******title********/
.title-text {
  color: #B20000;
}
.title-text .seach-button {
  margin-right: 0px;
}
.title-text .seach-button {
  background: #B20000 !important;
  color: #fff;
  border-color: #B20000 !important;
}
/******单选样式******/
.select-radio a {
  white-space: nowrap;
  width: auto!important;
  min-width: 10px;
  padding: 1px 9px;
  text-align: center;
  border: 1px solid #cccccc;
  color: #000;
  text-decoration: none;
  display: block;
  height: 30px;
  line-height: 27px;
  margin-bottom: 5px;
  margin-right: 5px;
}
.select-radio a:hover,
.select-radio .active {
  border: 2px solid #be0106;
  padding: 0px 8px;
}
/******提示窗口******/
.look-up {
  position: absolute;
  top: 40%;
  left: 35%;
  background: #000000;
  background: rgba(0, 0, 0, 0.4);
  display: none;
  z-index: 1050;
  /**大于弹出框**/

  height: 100px;
  width: 500px;
  padding: 10px;
}
.look-up-content {
  border-top: 3px solid #B20000;
  position: relative;
  background: #FFFFFF;
  width: 100%;
  height: 100%;
  padding: 10px;
  padding-top: 15px;
  overflow: auto;
}
.look-up .i-close {
  position: absolute;
  top: 6px;
  right: 10px;
  font-size: 20px;
  cursor: pointer;
  color: #B20000;
}
.look-up .text-success {
  font-weight: bold;
  font-size: 14px;
}
.look-up .i-close:hover {
  color: #B20000;
}
