}
.product-order .active {
  background-color: #ffebe8;
}
.product-order .active .iconfont {
  display: inline-block;
}
/*************floor**********/
.floor {
  font-size: 12px;
  margin-top: 10px;
  margin-bottom: 5px;
  line-height: 24px;
  color: #8c8c8c;
}
.floor-title {
  color: #555555;
  font-size: 12px;
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-weight: 500;
  border-left: 3px solid #ff4a00;
  padding-left: 6px;
}
.floor-title .iconfont {
  margin-top: -2px;
}
.floor-link a {
  color: #8c8c8c;
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
}
.floor-link a:hover {
  color: #333;
}
.floor-link span {
  padding: 0px 5px 0px 5px;
  color: #dfdfdf;
}
.floor-content {
  border: 1px solid #dfdfdf;
  border-right: 0px;
  border-bottom: 0px;
  background: #FFFFFF;
}
.floor-content > .product {
  border: 1px solid #dfdfdf;
  border-left: 0px;
  margin-top: -1px;
  height: 340px;
  line-height: 26px;
}
.floor-content > .product > .product-content {
  height: 100%;
  padding: 4px;
  border: 6px solid #fff;
  -webkit-transition: border-color 0.5s;
  transition: border-color 0.5s;
  overflow: hidden;
}
.product:hover > .product-content {
  border-color: #f3f3f3;
  border-color: rgba(243, 243, 243, 0.8);
}
.product .title {
  display: block;
  padding-top: 10px;
  padding-left: 20px;
}
.product a {
  color: #000;
}
.product a:hover {
  color: #ff4a00;
}
.product img {
  width: 230px;
  height: 230px;
}
.product .product-img {
  text-align: center;
  display: block;
  margin-bottom: 15px;
  margin-top: 10px;
}
/***********品牌**************/
.product-brand {
  background-color: #F7F5F5;
}
.brand-title {
  display: block;
  height: 30px;
  line-height: 30px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  padding-left: 10px;
  color: #B0A59F;
  border-top: 1px dotted #E6E2E1;
}
.brand-content {
  border-top: 1px dotted #E6E2E1;
  border-left: 1px dotted #E6E2E1;
  padding: 10px;
  background-color: #FFFFFF;
  min-height: 30px;
}
.brand-content .active {
  border: 1px solid #B20000 !important;
  position: relative;
}
.product-brand li {
  border: 1px solid #e1e1e1;
  width: 84px;
  text-align: center;
  height: 42px;
  line-height: 40px;
  overflow: hidden;
  margin: 0;
  padding: 0px;
  margin-right: -1px;
  margin-top: -1px;
  float: left;
}
.brand-content img {
  height: 40px;
}
.brand-content .name {
  display: none;
}
.product-brand a {
  text-decoration: none;
  color: #806F66;
  font-size: 12px;
  display: inline-block;
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
}
.brand-content > .active {
  border-color: #ffebe8 !important;
  background-color: #ffebe8;
