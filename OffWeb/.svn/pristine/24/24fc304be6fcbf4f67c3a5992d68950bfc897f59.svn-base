/***************手机导航******************/
/* wrap */
.phone-wrap {
  width: 46px;
  height: 46px;
  position: fixed;
  right: 10px;
  bottom: 10px;
  z-index: 1002;
}
.phone-wrap div {
  text-align: center;
  cursor: pointer;
  position: absolute;
  color: #fff;
}
/*h-shadow v-shadow blur spread color*/
.phone-menu {
  width: 46px;
  height: 46px;
  bottom: 10px;
  right: 10px;
  line-height: 46px;
  box-shadow: 0px 0px 2px 3px #B20000;
  background: #B20000;
  z-index: 10;
}
.btn-phone {
  bottom: 13px;
  right: 13px;
  width: 40px;
  height: 40px;
  line-height: 40px;
  box-shadow: 0px 0px 2px 2px #ffcc33;
  z-index: 1;
  background: rgba(255, 0, 0, 0.7);
  color: #fff!important;
  overflow: hidden;
}
.btn-phone a {
  color: #fff;
  font-size: 12px;
}
.btn-phone {
  -webkit-transition: bottom .2s,right .2s;
  -moz-transition: bottom .2s,right .2s;
  -o-transition: bottom .2s,right .2s;
  -ms-transition: bottom .2s,right .2s;
  transition: bottom .2s,right .2s;
}
.btn1.phone-close,
.btn2.phone-close,
.btn3.phone-close,
.btn4.phone-close,
.btn5.phone-close,
.btn6.phone-close {
  bottom: 13px;
  right: 13px;
}
