  -khtml-border-radius: 2px;
}
.product-brand li a {
  padding: 0px;
  line-height: 40px;
  height: 40px;
  width: 82px;
  overflow: hidden;
  border: 1px solid transparent;
  _border: 1px solid #fff;
}
.product-brand li:hover,
