body{
	font: 12px/1.5 arial,"Microsoft Yahei","Hiragino Sans GB",sans-serif;
	background:#ebebeb;
}
a{text-decoration:none;color:#333;cursor:pointer;}
a:hover{text-decoration:none;color:#999;}

/*************顶部导航条：top-bar************/
.top-bar {
	min-height: 30px;
	line-height: 30px;
	color: #fff;
	background: #000;
}

.top-bar .iconfont {
	display: inline-block !important;
	margin-top: -3px;
}

.top-bar span {
	padding: 0px 10px 0px 10px;
	color: #666;
}
.logo{max-height:40px;}
/*******************菜单*********************/
.navbar-inverse {
	margin-bottom: 0px;
	margin-right: 0px;
}
.navbar-static-top {
	border: 0px !important;
}
.navbar-nav {
  margin: 0px!important;
}

/****************图片滚动*******************/
.slide-div {
	position: absolute;
	background-repeat: repeat;
	top:0px; left:0px;
	/*background-image: url('../images/jpp-video-pattern.png');*/
	height: 100%;width: 100%;position: absolute; z-index:1; opacity: 0.6;
}
.slide-img img {
		width: auto!important;
		min-width: 100%!important;
		min-height: 100%!important;
		height:auto;
}
.index-slide-text{
	position:absolute;
	bottom:80px;
	color: #fff;
	font-size:22px;
	background-color: rgba(0, 0, 0, 0.2);
	left:0px;
	right:0px;
	margin: auto;
	max-width: 60%;
	text-align: center;
	overflow:hidden;
	padding:20px;
	z-index:3;
}
.carousel-inner, .item {
	height: 100%!important;
}
.carousel-inner {
	position: relative;
	width: 100%;
	height: 100%!important;
	overflow: hidden;
}
/***************网站详情页面*****************/
.news-detail-title {
	padding: 15px;
	border: 1px solid #eee;
	border-radius: 3px;
	border-left-width: 5px;
	border-left-color: #5bc0de;
	background: #f9f9f9;
	font-size: 18px;
	font-weight: bold;
	color: #555;
}
@media (max-width: 800px){
	#carousel-example-generic {
		height:auto!important;
	}
	.slide-img img {
		width: 100%!important;
		min-height: 0px!important;
		height:auto!important;
	}
	.index-slide-text{
		font-size:14px;
		bottom:50px;
		padding:15px;
	}

}











