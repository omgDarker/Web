<%@ page language="java" import="java.util.*" pageEncoding="utf-8"%>
<div class="title-text">
	<div class="container">
		<div class="row pt20 pb20">
			<div class="col-xs-12 col-sm-6 p0 tl">
						<div class="f30 fb hidden-xs">${pageTitle}</div>
						<div class="f18">${pageMessage}</div>
			</div>
			<form method="post" action="search" onsubmit="return iUserName('p_keyWord','搜索至少为2个字,不能包含特殊字符');">
			<div class="col-xs-12 col-sm-6  p0 pt10 tr input-group search-section">
				
								<input type="text" class="form-control seach-input ns" id="p_keyWord" value="${p_keyWord}"
									name="p_keyWord" placeholder="输入搜索关键字"> <span
									class="input-group-btn">
									<button class="btn btn-default seach-button" type="submit">
										<i class="iconfont">&#xe602;</i>
									</button> </span>
			</div>
			</form>
		</div>
	</div>
</div>